@model IEnumerable<INEC3.Models.tbl_Circonscription>

@{
    ViewBag.Title = "Index";
}

@Scripts.Render("~/bundles/jquery")
<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.css" rel="stylesheet" />
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Province.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Territoire.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enroles)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SiegesProv)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SiegesNat)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Province.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Territoire.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enroles)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SiegesProv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SiegesNat)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID_Circonscription }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID_Circonscription }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID_Circonscription })
            </td>
        </tr>
    }

</table>
