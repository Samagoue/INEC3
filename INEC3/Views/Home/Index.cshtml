@{
    ViewBag.Title = "Home Page";
}
@Styles.Render("~/Content/zoomMap.css")
@Scripts.Render("~/bundles/jquery")
<style>
    .deactive {
        fill: #444 !important;
    }

    .datamaps-subunit:hover {
        stroke-width: 2px;
    }

    .state {
        stroke: rgb(128, 128, 128);
        stroke-linejoin: round;
    }

    .eln-popup {
        /*text-align: left;*/
        position: absolute;
        background-color: white;
        border: 1px solid #ccc;
        z-index: 1000001;
        box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.5);
        pointer-events: none;
    }

        .eln-popup .eln-title {
            font-size: 15px;
            font-weight: 700;
            margin-bottom: 3px;
        }

        .eln-popup .eln-default-subtitle {
            color: #999;
            font: 400 11px/1 nyt-franklin, arial, helvetica, sans-serif;
        }

        .eln-popup .eln-popup-content {
            padding: 6px 8px;
            font: 13px/1.3 nyt-franklin, arial, helvetica, sans-serif;
            white-space: nowrap;
        }

        .eln-popup .eln-has-electoral-votes .eln-subtitle {
            position: absolute;
        }

        .eln-popup .eln-subtitle {
            color: #666;
            font: 400 12px/1 nyt-franklin, arial, helvetica, sans-serif;
            margin: 0 0 8px;
        }

        .eln-popup .eln-results-container {
            padding-bottom: 0 !important;
        }

    .eln-results-container {
        position: relative;
        padding-bottom: 20px;
        -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    }

        .eln-results-container .eln-results-table {
            font-size: 17px;
            width: 100%;
            font: 400 13px/1.25 nyt-franklin, arial, helvetica, sans-serif;
            margin-bottom: 8px;
            -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        }

    .eln-popup .eln-has-electoral-votes .eln-results-row-president .eln-header {
        padding-top: 3px;
    }

    .eln-popup .eln-results-container .eln-name {
        padding-top: 0;
        padding-left: 0 !important;
        padding-right: 0;
    }

    .eln-popup .eln-results-container .eln-header {
        font-size: 11px;
        padding-bottom: 4px;
    }

    .eln-results-container .eln-name {
        text-align: left;
    }

    .eln-results-container .eln-header {
        font: 400 11px/1 nyt-franklin, arial, helvetica, sans-serif;
        text-transform: uppercase;
        padding-bottom: 5px;
        border-bottom: 1px solid #e2e2e2;
        vertical-align: bottom;
        /*color: #999;*/
    }

    .eln-results-container .eln-row {
        border-bottom: 1px solid #e2e2e2;
    }


    tr {
        display: table-row;
        vertical-align: inherit;
        border-color: inherit;
    }

    .eln-results-container .eln-results-table {
        width: 100%;
        font: 400 13px/1.25 nyt-franklin, arial, helvetica, sans-serif;
        margin-bottom: 8px;
        -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    }

    table {
        width: 96%;
        border-collapse: collapse;
    }

    .user agent stylesheet
    table {
        display: table;
        border-collapse: separate;
        border-spacing: 2px;
        border-color: grey;
    }

    .eln-results-container {
        position: relative;
        padding-bottom: 20px;
        -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    }

    .eln-popup .eln-popup-content {
        padding: 6px 8px;
        font: 13px/1.3 nyt-franklin, arial, helvetica, sans-serif;
        white-space: nowrap;
    }

    .map-container {
        -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        overflow: hidden;
        position: relative;
    }



    .eln-shell {
        width: 100%;
        max-width: 1600px;
        min-height: 100vh;
        margin: 0 auto;
        position: relative;
    }

    .eln-map-buttons {
        cursor: pointer;
        z-index: 200;
        /*position: absolute;*/
        top: 5px;
        left: 10px;
        display: block;
        border-radius: 50%;
        margin-bottom: 6px;
        font-size: 30px;
    }

        .eln-map-buttons:disabled {
            font-size: 20px !important;
        }

    .two-part li span {
        font-size: 30px !important;
    }

    .list-inline li span {
        font-size: 30px !important;
    }

    .key {
        color: #242425;
    }

    .padding-no-lr {
        padding-left: 0px !important;
        padding-right: 0px !important;
    }
</style>
<div class="container-fluid">
    <div class="row bg-title">
        <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
            <h4 class="page-title">Shadow Dashboard</h4>
        </div>
        <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
            <ol class="breadcrumb">
                <li><a href="@Url.Action("Index","Home")">Home</a></li>
                <li class="active">Index</li>
            </ol>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
            <div class="white-box responsive">
                <div class="row text-center">
                    <div class="col-md-3 col-sm-4 col-xs-6 padding-no-lr">
                        <img src="~/Content/image/logo.png" alt="user" id="imgcandidate0" class="img-circle img-responsive img-top-candidate">
                    </div>
                    <div class="col-md-9 col-sm-8 col-xs-6 padding-no-lr" style="padding-left: 20px !important;">
                        <h4 class="text-center"><span id="lblcandidate0" class="counter"></span></h4>
                        <h5><span id="lblcandidateVote0" class="counter">0</span><small> ( <span id="lblcandidatePer0" class="counter">0 %</span> )</small></h5>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
            <div class="white-box responsive">
                <div class="row text-center">
                    <div class="col-md-3 col-sm-4 col-xs-6 padding-no-lr">
                        <img src="~/Content/image/logo.png" alt="user" id="imgcandidate1" class="img-circle img-responsive img-top-candidate">
                    </div>
                    <div class="col-md-9 col-sm-8 col-xs-6 padding-no-lr" style="padding-left: 20px !important;">
                        <h4 class="text-center"><span id="lblcandidate1" class="counter"></span></h4>
                        <h5><span id="lblcandidateVote1" class="counter">0</span><small> ( <span id="lblcandidatePer1" class="counter">0 %</span> )</small></h5>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
            <div class="white-box responsive">
                <h5 class="box-title">Reported Polling Stations</h5>
                <ul class="list-inline two-part" @*style="margin-bottom: -8px;"*@>
                    <li class="text-left"><span id="ReportPolstation" class="counter">0</span> Reported</li>
                    <li class="text-right"><span id="PersPolstation" class="counter">0</span></li>
                </ul>
            </div>
        </div>

        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
            <div class="white-box responsive">
                <h5 class="box-title">Last Reported Pol. Stat.</h5>
                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6 text-center"><label id="lbllastupdatedVotantsName" style="font-weight: 300 !important;"></label><br /><span id="lbllastupdatedVotants" style="font-size: 20px !important;"></span></div>

                    <div class="col-md-6 col-sm-6 col-xs-6 text-center"> <label id="lbllastupdatedProvinceName" style="font-weight: 300 !important;"></label> <br /><span id="lbllastupdatedProvince" style="font-size: 20px !important;"></span></div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-8 col-md-12">
            <div id="mapcontainer" class="white-box">
                <div id="map" style="height: 410px"></div>
                <div style="cursor: pointer;z-index:200;position:absolute;top:25px;left:41px;">
                    <i class="mdi mdi-magnify-plus eln-map-buttons" id="btnzoomin"></i>
                    <i class="mdi mdi-magnify-minus eln-map-buttons" id="btnzoomout"></i>
                    <i class="mdi mdi-close-circle eln-map-buttons" id="btncancel" onclick="activearea()" disabled></i>
                </div>
            </div>
        </div>

        <div class="col-lg-4 col-md-6">
            <div class="white-box">
                <h3 class="box-title" id="LblToltiptitle">National Results</h3>
                <ul class="country-state  p-t-20" id="TooltipChart">
                   
                </ul>
            </div>
        </div>
    </div>
</div>
<div id="nvtable" class="eln-popup" hidden>
    <div class="eln-title  eln-results-row-president" style="margin-top:10px;margin-left:5px;"><label id="header"> Alabama </label><span id="lblreporting" class="eln-default-subtitle" style="color:#6f6d6d !important;float: right; margin-right: 2%;"> 0% Reporting </span></div>
    <table class="eln-table eln-results-table">
        <thead>
            <tr style="border-bottom: 1px solid #181819;">
                <th class="eln-header eln-name" colspan="3" style="color:#383636 !important;"><strong>Candidate</strong></th>
                <th style="color:#383636 !important;"><strong>Party</strong></th>
                <th style="color:#383636 !important;"><strong>Votes</strong></th>
                <th style="color:#383636 !important;"><strong>Pct.</strong></th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
<footer class="footer text-center">
    Copyright © 2018 <a href="http://www.artechconsulting.us">Artech Consulting</a>. All Rights Reserved.
</footer>
@section Scripts {
    @Scripts.Render("~/bundles/jqud3mapHome")
    @Scripts.Render("~/bundles/HomeCustomjs")
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.signalR-2.3.0.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/signalr/hubs")"></script>
    <script>
        $(function () { $(".preloader").fadeOut();});
        var rs =@Html.Raw(ViewBag.Province);
        var ProvinceResult = rs.Table, TerritoiresResult = rs.Table1, TopCandidate = rs.Table2, ReportPolStation = rs.Table3,LastUpdatedPoolstn=rs.Table4;
        $('#TooltipChart').slimScroll({
            height: '370px'
        });
    </script>
}
