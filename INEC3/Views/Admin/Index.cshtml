@model IEnumerable<INEC3.Models.ResultViewModel>
@{
    ViewBag.Title = "Result List";
}
<link href="@Url.Content("~/Content/plugins/bower_components/datatables/media/css/dataTables.bootstrap.css")" rel="stylesheet" type="text/css" />
@Scripts.Render("~/bundles/jquery")
@if (ViewBag.Message != null)
{
    <div class="alert alert-danger alert-dismissable">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button><b>Error </b>@ViewBag.Message
    </div>
}
<div class="container-fluid">
    <div class="row bg-title">
        <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
            <h4 class="page-title">Result</h4>
        </div>
        <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
            <a href="@Url.Action("Result")" class="btn btn-danger pull-right m-l-20  waves-effect waves-light">Add New</a>
            <ol class="breadcrumb">
                <li><a href="#">Result</a></li>
                <li class="active">List</li>
            </ol>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /row -->
    <div class="row">
        <div class="col-sm-12">
            <div class="white-box">
                <h3 class="box-title m-b-0">Result List</h3>
                <div class="table-responsive">
                    <table id="myTable" class="table display">
                        <thead>
                            <tr>
                                <th>@Html.DisplayName("Province")</th>
                                <th>@Html.DisplayName("Territoire")</th>
                                <th>@Html.DisplayName("Commune")</th>
                                <th>@Html.DisplayName("Polling Station")                                </th>
                                <th>@Html.DisplayName("CODE_SV")</th>
                                <th>@Html.DisplayName("Candidat")</th>                                
                                <th>@Html.DisplayName("Party")</th>                                
                                <th>@Html.DisplayName("Voix")</th>                                
                                <th>@Html.DisplayName("Pourcentage")</th>                                
                                <th>@Html.DisplayName("Exprimes")</th>
                            </tr>
                        </thead>

                        <tbody>
                            @foreach (var item in Model)
                            {
                               
                                <tr>
                                    <td>@Html.DisplayFor(modelItem => item.Province)    </td>
                                    <td>@Html.DisplayFor(modelItem => item.Territoire)                                        </td>
                                    <td>@Html.DisplayFor(modelItem => item.Commune)                                        </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PolStation)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Code_SV)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Candidat)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.Party)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Voix)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Pourcentage) %
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Exprimes)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<footer class="footer text-center">
    Copyright Â© 2018 <a href="http://www.artechconsulting.us" target="blank">Artech Consulting</a>. All Rights Reserved.
</footer>

@section Scripts {
    <!-- Custom Theme JavaScript -->
    <script src="~/Content/plugins/bower_components/datatables/datatables.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $(".preloader").fadeOut();
            $('#myTable').DataTable();
        });
    </script>
}
